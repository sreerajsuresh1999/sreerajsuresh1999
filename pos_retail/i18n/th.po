#. module: pos_retail
#: model:mail.template,body_html:pos_retail.email_template_pos_ereceipt
#, fuzzy
msgid ""
"\n"
"                    <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); "
"background-color: #FFF; \">\n"
"\n"
"                        <p>Hello ${object.partner_id and object.partner_id.name or ''},</p>\n"
"\n"
"                        <p style=\"border-left: 1px solid #8e0000; margin-left: 30px;\">\n"
"                           &nbsp;&nbsp;<strong>REFERENCES</strong><br />\n"
"                           &nbsp;&nbsp;Order number: <strong>${object.name}</strong><br />\n"
"                           &nbsp;&nbsp;Order total: <strong>${object.amount_total} ${object.company_id.currency_id.name}</strong><br />\n"
"                           &nbsp;&nbsp;Order date: ${object.date_order}<br />\n"
"                           % if object.user_id:\n"
"                           &nbsp;&nbsp;Cashier: ${object.user_id.name}\n"
"                           % endif\n"
"                        </p>\n"
"                        <p> Product Details </p>\n"
"                        % if object.lines:\n"
"                            <table border=1>\n"
"                                <tbody border=1>\n"
"                                <tr>\n"
"                                    <th>Product Name</th>\n"
"                                    <th>Quantity</th>\n"
"                                    <th>Unit Price</th>\n"
"                                    <th>Discount %</th>\n"
"                                    <th>Total(${object.company_id.currency_id.name})</td>\n"
"                                <tr>\n"
"                                % for line in object.lines:\n"
"                                    <tr>\n"
"                                        <td>${line.product_id.name}</td>\n"
"                                        <td style=\"text-align:right\">${line.qty}</td>\n"
"                                        <td style=\"text-align:right\">${line.price_unit}</td>\n"
"                                        <td style=\"text-align:right\">${line.discount}</td>\n"
"                                        <td style=\"text-align:right\">${line.price_subtotal_incl}</td>\n"
"                                    </tr>\n"
"                                 % endfor\n"
"                                 <tr>\n"
"                                    <td colspan=\"4\"><strong>Tax</strong></td>\n"
"                                    <td style=\"text-align:right\">${object.amount_tax}</td>\n"
"                                 </tr>\n"
"                                 <tr>\n"
"                                    <td colspan=\"4\"><strong>Total Amount</strong></td>\n"
"                                    <td style=\"text-align:right\">${object.amount_total}</td>\n"
"                                 </tr>\n"
"                                 </tbody>\n"
"                            </table>\n"
"                        % endif\n"
"                        <br/>\n"
"                        <p>Thank you for choosing ${object.company_id.name or 'us'} !</p>\n"
"                        <br/>\n"
"                        <br/>\n"
"                        <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; "
"border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"                            <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n"
"                                <strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3>\n"
"                        </div>\n"
"                        <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"                            <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                            % if object.company_id.street:\n"
"                                ${object.company_id.street}<br/>\n"
"                            % endif\n"
"                            % if object.company_id.street2:\n"
"                                ${object.company_id.street2}<br/>\n"
"                            % endif\n"
"                            % if object.company_id.city or object.company_id.zip:\n"
"                                ${object.company_id.zip} ${object.company_id.city}<br/>\n"
"                            % endif\n"
"                            % if object.company_id.country_id:\n"
"                                ${object.company_id.state_id and ('%s, ' % object.company_id.state_id.name) or ''} ${object.company_id."
"country_id.name or ''}<br/>\n"
"                            % endif\n"
"                            </span>\n"
"                            % if object.company_id.phone:\n"
"                                <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; "
"padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                                    Phone:&nbsp; ${object.company_id.phone}\n"
"                                </div>\n"
"                            % endif\n"
"                            % if object.company_id.website:\n"
"                                <div>\n"
"                                    Web :&nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n"
"                                </div>\n"
"                            %endif\n"
"                            <p></p>\n"
"                        </div>\n"
"                    </div>\n"
"                "
msgstr ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: "
"#FFF; \"><p>สวัสดี ${object.partner_id และ object.partner_id.name หรือ &#39;&#39;}</p><p style=\"border-left: 1px solid #8e0000; margin-left: "
"30px;\"> <strong>ข้อมูลอ้างอิง</strong><br /> หมายเลขคำสั่งซื้อ: <strong>${object.name}</strong><br /> ยอดสั่งซื้อทั้งหมด: <strong>${object.amount_total} "
"${object.company_id.currency_id.name}</strong><br /> วันที่สั่งซื้อ: ${object.date_order}<br /> % if object.user_id: แคชเชียร์: ${object.user_id.name} "
"% endif</p><p> รายละเอียดสินค้า</p> % if object.lines:<table border=1><tbody border=1><tr><th> ชื่อผลิตภัณฑ์</th><th> ปริมาณ</th><th> ราคาต่อหน่วย</"
"th><th> ส่วนลด%</th><th> ทั้งหมด(${object.company_id.currency_id.name})</td><tr> % fหรือบรรทัดใน object.lines:<tr><td> ${line.product_id.name}</"
"td><td style=\"text-align:right\"> ${line.qty}</td><td style=\"text-align:right\"> ${line.price_unit}</td><td style=\"text-align:right\"> "
"${line.discount}</td><td style=\"text-align:right\"> ${line.price_subtotal_incl}</td></tr> % eสำหรับ<tr><td colspan=\"4\"> <strong>ภาษี</"
"strong></td><td style=\"text-align:right\"> ${object.amount_tax}</td></tr><tr><td colspan=\"4\"> <strong>จำนวนเงินทั้งหมด</strong></td><td style="
"\"text-align:right\"> ${object.amount_total}</td></tr></tbody></table> % eนดิฟ<br/><p> ขอขอบคุณที่เลือก ${object.company_id.name or &#39;us&#39;} !"
"</p><br/><br/><div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-"
"right-radius: 5px 5px; background-repeat: repeat no-repeat;\"><h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\"> "
"<strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3></div><div style=\"width: 347px; margin: 0px; padding: 5px "
"14px; line-height: 16px; background-color: #F2F2F2;\"> <span style=\"color: #222; margin-bottom: 5px; display: block; \">% if object."
"company_id.street: ${object.company_id.street}<br/> % eและ % if object.company_id.street2: ${object.company_id.street2}<br/> % endif % if "
"object.company_id.city หรือ object.company_id.zip: ${object.company_id.zip} ${object.company_id.city}<br/> % endif % if object.company_id."
"country_id: ${object.company_id.state_id and (&#39;%s, &#39; % object.company_id.state_id.name) หรือ &#39;&#39;} ${object.company_id. "
"country_id.name หรือ &#39;&#39;}<br/> % eและ</span> % if object.company_id.phone:<div style=\"margin-top: 0px; margin-right: 0px; margin-"
"bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \"> โทรศัพท์: ${object.company_id."
"phone}</div> % endif % if object.company_id.website:<div> เว็บ : <a href=\"${object.company_id.website}\">${object.company_id.website}</a></"
"div> %eครั้งต่อครั้ง<p></p></div></div>"

