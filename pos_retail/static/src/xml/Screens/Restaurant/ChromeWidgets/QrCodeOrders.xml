<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="QrCodeOrders" owl="1">
        <div class="ticket-button" t-att-class="{ oe_hidden: isHidden}" t-on-click="onClick">
            <div class="with-badge" t-att-badge="count">
                <i class="fa fa-ticket" aria-hidden="true"></i>
            </div>
            <div t-if="!env.isMobile">QRCode</div>
        </div>
    </t>

</templates>
