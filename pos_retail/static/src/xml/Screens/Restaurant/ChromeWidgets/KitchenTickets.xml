<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="KitchenTickets" owl="1">
        <div class="ticket-button" t-att-class="{ oe_hidden: isHidden, animation: count  &gt; 0}" t-on-click="onClick">
            <div class="with-badge" t-att-badge="count">
                <i class="fa fa-cutlery" aria-hidden="true"></i>
            </div>
            <div>KOT</div>
        </div>
    </t>

</templates>
