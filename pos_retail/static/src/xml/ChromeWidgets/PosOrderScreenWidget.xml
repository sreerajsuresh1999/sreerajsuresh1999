<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="PosOrderScreenWidget" owl="1">
        <div class="ticket-button pos-orders-header-button" t-att-class="{ oe_hidden: isHidden}" t-on-click="onClick">
            <div class="with-badge" t-att-badge="count">
                <i class="fa fa-list" aria-hidden="true"></i>
            </div>
            <div t-if="!env.isMobile" class="header-text">POS Orders</div>
            <span class="shortcut-keyboard" t-if="!env.isMobile"> <i class="fa fa-keyboard-o"/>  F11 </span>
        </div>
    </t>

</templates>
