<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="GiftCardsWidget" owl="1">
        <div class="ticket-button invoices-header-button" t-att-class="{ oe_hidden: isHidden}" t-on-click="onClick">
            <div class="with-badge" t-att-badge="count">
                <i class="fa fa-gift" aria-hidden="true"></i>
            </div>
            <div t-if="!env.isMobile" class="header-text">Gift Cards</div>
        </div>
    </t>

</templates>
